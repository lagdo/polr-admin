{
    "name": "lagdo/polr-admin",
    "description": "An admin dashboard for the Polr URL shortener.",
    "keywords": ["framework", "laravel"],
    "license": "GPL-v2",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "yajra/laravel-datatables-oracle": "~6.0",
        "nesbot/carbon": "^1.22",
        "guzzlehttp/guzzle": "~6.0",
        "jaxon-php/jaxon-laravel": "~2.0",
        "jaxon-php/jaxon-dialogs": "~1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "App\\": "app/",
            "Lagdo\\Polr\\Admin\\": "src/",
            "Lagdo\\Polr\\Admin\\App\\": "src/Ajax/Classes/",
            "Lagdo\\Polr\\Admin\\Ext\\": "src/Ajax/Plugins/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
